{
    "models": [
        {
            "model": "gpt-3.5-turbo",
            "provider": "openai"
        },
        {
            "model": "claude-3-sonnet-20240229",
            "provider": "anthropic"
        }
    ],
    "prompts": [
        "Find all yellow fruits from this list: {fruits}",
        "Extract the yellow-colored fruits: {fruits}",
        "Identify yellow fruits in: {fruits}"
    ],
    "temperature": [0.0, 0.1, 0.3],
    "output_schema": {
        "type": "object",
        "properties": {
            "yellow_fruits": {
                "type": "array",
                "items": {"type": "string"}
            }
        }
    },
    "scenarios": [
        {
            "input": {
                "fruits": ["apples", "bananas", "lemons", "oranges"]
            },
            "expected_output": {
                "yellow_fruits": ["bananas", "lemons"],
                "confidence": 0.95
            },
            "description": "Basic fruit identification"
        },
        {
            "input": {
                "fruits": ["grapes", "pineapple", "strawberries"]
            },
            "expected_output": {
                "yellow_fruits": ["pineapple"],
                "confidence": 0.9
            },
            "description": "Mixed fruit test"
        }
    ]
}
